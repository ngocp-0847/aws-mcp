{
  "name": "mcp-aws",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "debug": "tsx --inspect src/index.ts",
    "debug-brk": "tsx --inspect-brk src/index.ts",
    "test-tool": "tsx scripts/test-tool.ts",
    "health-check": "tsx scripts/health-check.ts",
    "logs": "tail -f debug.log"
  },
  "dependencies": {
    "@aws-sdk/client-athena": "^3.632.0",
    "@aws-sdk/client-cloudwatch": "^3.876.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.632.0",
    "@aws-sdk/client-cost-explorer": "^3.632.0",
    "@aws-sdk/client-ecr": "^3.632.0",
    "@aws-sdk/client-ecs": "^3.632.0",
    "@aws-sdk/client-pi": "^3.876.0",
    "@aws-sdk/client-rds": "^3.876.0",
    "@aws-sdk/client-rds-data": "^3.632.0",
    "@aws-sdk/client-s3": "^3.632.0",
    "@aws-sdk/client-sts": "^3.632.0",
    "@modelcontextprotocol/sdk": "^1.17.4",
    "dotenv": "^16.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  }
}
